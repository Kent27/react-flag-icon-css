{
  "name": "react-flag-icon-css",
  "description": "React SVG country flags component",
  "homepage": "https://github.com/matteocng/react-flag-icon-css#readme",
  "version": "1.0.7",
  "main": "./lib",
  "module": "./module",
  "author": {
    "email": "matteocng+npmjs@outlook.com",
    "name": "matteocng",
    "url": "https://github.com/matteocng"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteocng/react-flag-icon-css.git"
  },
  "bugs": {
    "url": "https://github.com/matteocng/react-flag-icon-css/issues"
  },
  "keywords": [
    "react",
    "flag-icon",
    "flag-icons",
    "flag-icon-css",
    "react-css-modules"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "flag-icon-css": "^2.8.0",
    "react-css-modules": "^4.1.0",
    "tcomb": "^3.2.16",
    "tcomb-react": "^0.9.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "classnames": "^2.2.5",
    "cpy-cli": "^1.0.1",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flag-icon-css": "^2.8.0",
    "flow-bin": "^0.39.0",
    "flow-copy-source": "^1.1.0",
    "jsdom": "^9.11.0",
    "mkdirp": "^0.5.1",
    "postcss-at-rules-variables": "^0.0.27",
    "postcss-cli": "^3.0.0-beta",
    "postcss-custom-properties": "^5.0.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-each": "^0.9.3",
    "react": "^16.0.0-alpha.2",
    "react-addons-test-utils": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^^16.0.0-alpha.2",
    "rimraf": "^2.5.4",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "tape": "^4.6.3",
    "tcomb": "^3.2.16",
    "tcomb-react": "^0.9.3"
  },
  "engines": {
    "node": ">=4"
  },
  "postcss": {
    "plugins": {
      "postcss-at-rules-variables": {},
      "postcss-each": {},
      "postcss-custom-properties": {},
      "postcss-discard-comments": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "each"
          ]
        }
      ]
    }
  },
  "scripts": {
    "build": "BABEL_ENV=build babel src --out-dir ./lib",
    "build:module": "BABEL_ENV=module babel src --out-dir ./module ",
    "build:watch": "BABEL_ENV=build babel src --watch --out-dir ./lib",
    "build:module:watch": "BABEL_ENV=module babel src --watch --out-dir ./module",
    "clean": "rimraf ./lib",
    "clean:module": "rimraf ./module",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint src/styles/main.css",
    "lint:js": "eslint src static cmrh.conf.js .eslintrc.js",
    "lint:js:fix": "eslint src static cmrh.conf.js --fix",
    "postbuild": "npm run styles && flow-copy-source --ignore *__tests__/* -v src lib",
    "postbuild:module": "npm run styles && flow-copy-source --ignore *__tests__/* -v src module",
    "posttest": "npm run lint && npm run flow",
    "prebuild": "npm run clean",
    "prebuild:module": "npm run clean:module",
    "prepublish": "npm run test && npm run build && npm run build:module",
    "styles": "postcss -o lib/styles/main.css src/styles/main.css && cpy lib/styles/main.css module/styles/",
    "test": "BABEL_ENV=testing tape -r babel-register -r babel-polyfill \"src/**/__tests__/index.js\""
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14 || ^15.0 || ^16.0",
    "react-dom": "^0.14 || ^15.0 || ^16.0"
  }
}
