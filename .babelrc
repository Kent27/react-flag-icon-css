{
	"presets": [
		"react"
	],
	"plugins": [
		"transform-object-rest-spread",
		"transform-export-extensions"
	],
	"comments": false,
	"env": {
		"build": {
			"presets": ["es2015"],
			"ignore": ["**/__tests__/"]
		},
		"module": {
			"presets": [
				["es2015", {
					"modules": false
				}]
			],
			"ignore": ["**/__tests__/"]
		},
		"testing": {
			"presets": ["es2015"]
		}
	}
}
